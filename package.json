{
  "name": "jupyter-services",
  "version": "0.0.1",
  "description": "Client APIs for the Jupyter services REST APIs",
  "main": "lib/index.js",
  "dependencies": {
    "js-logger": "^1.1.1",
    "phosphor-signaling": "^0.9.1"
  },
  "devDependencies": {
    "browserify": "11.0.1",
    "dts-bundle": "^0.3.0",
    "expect.js": "^0.3.1",
    "karma": "^0.12.36",
    "karma-chrome-launcher": "^0.1.12",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "mocha": "^2.2.5",
    "mock-socket": "^0.6.0",
    "rimraf": "^2.4.2",
    "sinon": "^1.15.4",
    "typedoc": "^0.3.8",
    "typescript": "^1.5.3"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf test/build",
    "build:src": "tsc --project src",
    "build:dts": "node scripts/dtsbundle.js",
    "build:test": "tsc --project test",
    "build": "npm run build:src && npm run build:dts && npm run build:test",
    "docs": "typedoc --out build/docs --name 'Jupyter JS Docs' --mode file --target ES6 --experimentalDecorators --includeDeclarations typings/text-encoding/ typings/es6-dataview.d.ts src/",
    "prepublish": "npm run build",
    "pretest": "browserify test/build/index.js --outfile test/build/app.js --debug",
    "test": "karma start test/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyter/jupyter-js-services"
  },
  "keywords": [
    "jupyter",
    "js",
    "services",
    "notebook"
  ],
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyter/jupyter-js-services/issues"
  },
  "typescript":{
    "definition": "lib/jupyter-js-services.d.ts"
  },
  "homepage": "https://github.com/jupyter/jupyter-js-services"
}
